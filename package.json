  "packageManager": "pnpm@8.15.6+sha256.01c01eeb990e379b31ef19c03e9d06a14afa5250b82e81303f88721c99ff2e6f",
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.6",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@mendable/data-connectors": "^0.0.48",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5"
  },
  "prettier": {
    "printWidth": 120,
